{
  "hosting": {
    "public": "dist",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "/**",
        "destination": "/static/index.html"
      },
      {
        "source": "/app/**",
        "destination": "/app/index.html"
      }
    ]
  }
}
